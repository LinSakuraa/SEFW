{"ast":null,"code":"import { defineComponent, ref } from \"vue\";\nexport default defineComponent({\n  methods: {\n    usernameOnBluer() {\n      console.log('usernameOnBluer');\n\n      if (this.username == '') {\n        this.wrong1 = true;\n        this.loginWrongText1 = '用户名不能为空';\n      } else {\n        this.wrong1 = false;\n        this.loginWrongText1 = '';\n      }\n    },\n\n    passwordOnBluer() {\n      console.log('passwordOnBluer');\n      var reg = /^(?=.*[0-9])(?=.*[a-zA-Z]).{6,16}$/;\n\n      if (this.password == '') {\n        this.wrong2 = true;\n        this.loginWrongText2 = '密码不能为空';\n      } else if (!reg.test(this.password)) {\n        this.wrong2 = true;\n        this.loginWrongText2 = '密码必须包含字母、数字、且位于6-16位之间';\n      } else {\n        this.wrong2 = false;\n        this.loginWrongText2 = '';\n      }\n    },\n\n    //以下为涉及axios调用的方法\n\n    /****************************************************************/\n    //ajax请求 方法\n    postUser(username, password) {\n      //axios发送AJAX请求\n      this.axios.post('/api/login', {\n        \"username\": username,\n        \"password\": password\n      }).then(response => {\n        console.log(response);\n      }).catch(response => {\n        console.log(response);\n      });\n    }\n\n  },\n\n  data() {\n    return {\n      username: '',\n      password: '',\n      wrong1: false,\n      wrong2: false,\n      loginWrongText1: '',\n      loginWrongText2: ''\n    };\n  },\n\n  setup() {\n    function funLogin() {\n      if (this.wrong1 === false && this.wrong2 === false) {\n        alert('username=' + this.username + 'password=' + this.password);\n        this.$store.state.username = this.username;\n        this.$store.state.password = this.password; //axios发送AJAX请求\n        //发送 this.username this.password\n\n        this.postUser(this.username, this.password); //调用postUser方法发送请求\n\n        this.$store.state.user.username = this.$store.state.username;\n        this.$store.state.user.password = this.$store.state.password;\n        this.$store.state.username = null;\n        this.$store.state.password = null; //请求接收完毕 登录成功\n\n        this.$store.state.isLogin = true;\n\n        if (this.$store.state.isLogin == true) {\n          console.log(this.$store.state.isLogin);\n          if (this.$store.state.user.isAdmin == true) this.$router.replace(\"/Admin\");else {\n            this.$router.replace(\"/Customer\");\n          }\n        } else {\n          console.log('登录失败!');\n        } //登录失败\n\n      }\n    }\n\n    return {\n      funLogin,\n      bodyStyle: {\n        width: \"600px\"\n      },\n      segmented: {\n        content: \"soft\",\n        footer: \"soft\"\n      },\n      showModal: ref(false)\n    };\n  }\n\n});","map":{"version":3,"mappings":"AA0CA,SAASA,eAAT,EAA0BC,GAA1B,QAAqC,KAArC;AAEA,eAAeD,eAAe,CAAC;AAC7BE,SAAO,EAAC;AACNC,mBAAe,GAAE;AACfC,aAAO,CAACC,GAAR,CAAY,iBAAZ;;AACA,UAAG,KAAKC,QAAL,IAAe,EAAlB,EAAqB;AAAC,aAAKC,MAAL,GAAY,IAAZ;AAAiB,aAAKC,eAAL,GAAqB,SAArB;AAA8B,OAArE,MACK;AAAC,aAAKD,MAAL,GAAY,KAAZ;AAAkB,aAAKC,eAAL,GAAqB,EAArB;AAAwB;AAEjD,KANK;;AAONC,mBAAe,GAAE;AACfL,aAAO,CAACC,GAAR,CAAY,iBAAZ;AACA,UAAIK,GAAG,GAAE,oCAAT;;AACA,UAAG,KAAKC,QAAL,IAAe,EAAlB,EAAqB;AAAC,aAAKC,MAAL,GAAY,IAAZ;AAAiB,aAAKC,eAAL,GAAqB,QAArB;AAA6B,OAApE,MACK,IAAG,CAACH,GAAG,CAACI,IAAJ,CAAS,KAAKH,QAAd,CAAJ,EAA4B;AAAC,aAAKC,MAAL,GAAY,IAAZ;AAAiB,aAAKC,eAAL,GAAqB,wBAArB;AAA6C,OAA3F,MACD;AAAC,aAAKD,MAAL,GAAY,KAAZ;AAAkB,aAAKC,eAAL,GAAqB,EAArB;AAAwB;AAChD,KAbK;;AAeL;;AACD;AACA;AACAE,YAAQ,CAACT,QAAD,EAAUK,QAAV,EAAmB;AAAC;AAC1B,WAAKK,KAAL,CAAWC,IAAX,CAAgB,YAAhB,EACA;AAAC,oBAAWX,QAAZ;AAAqB,oBAAWK;AAAhC,OADA,EAEGO,IAFH,CAESC,QAAD,IAAY;AAClBf,eAAO,CAACC,GAAR,CAAac,QAAb;AAED,OALD,EAKGC,KALH,CAKUD,QAAD,IAAY;AACnBf,eAAO,CAACC,GAAR,CAAac,QAAb;AACD,OAPD;AAQF;;AA3BM,GADqB;;AA8B7BE,MAAI,GAAE;AACJ,WAAM;AACNf,cAAQ,EAAC,EADH;AAENK,cAAQ,EAAC,EAFH;AAGNJ,YAAM,EAAC,KAHD;AAINK,YAAM,EAAC,KAJD;AAKNJ,qBAAe,EAAC,EALV;AAMNK,qBAAe,EAAC;AANV,KAAN;AAQD,GAvC4B;;AAwC7BS,OAAK,GAAG;AACJ,aAASC,QAAT,GAAmB;AACjB,UAAG,