{"ast":null,"code":"import { defineComponent } from \"vue\";\nexport default defineComponent({\n  name: 'AdminSearch',\n  props: {},\n\n  data() {\n    return {\n      buttonText: '书名',\n      inputText: '',\n      booklist: [],\n      userlist: [],\n      showSearch: false,\n      showUser: false\n    };\n  },\n\n  component: {},\n  watch: {},\n  methods: {\n    handleSelect(key) {\n      this.buttonText = key;\n    },\n\n    submitInput() {\n      if (this.buttonText == '书名') {\n        this.searchName(this.inputText);\n      } else if (this.buttonText == '类型') {\n        this.searchTag(this.inputText);\n      } else if (this.buttonText == '账号') {\n        this.searchUser(this.inputText);\n      }\n    },\n\n    //以下为涉及axios调用的方法\n\n    /****************************************************************/\n    //ajax请求 方法\n    searchName(Name) {\n      //方法：get  发送Name 返回对象数组\n      //ajax request: search bookName\n      this.Userid = [];\n      this.booklist = []; // 清空当前显示的数据\n\n      {\n        //axios发送AJAX请求\n        this.axios.get('', {\n          \"Name\": Name\n        }).then(response => {\n          console.log(response);\n        }).catch(response => {\n          console.log(response);\n        });\n      } //1.成功搜索到结果\n      //获取对象数组 显示相应结果\n\n      this.showSearch = true;\n      this.booklist.push({\n        bookid: 55,\n        bookname: Name,\n        bookauthor: 'author' + 1,\n        bookpublisher: 'publisher' + 1,\n        booktag: 'amusement'\n      }); //2.未找到\n      // this.showSearch=false;//不显示结果区域\n      //发送警告\n    },\n\n    searchTag(Tag) {\n      //方法：get  发送Name 返回对象数组\n      //ajax request  search bookTag\n      this.Userid = [];\n      this.booklist = []; // 清空当前显示的数据\n\n      {\n        //axios发送AJAX请求\n        this.axios.get('', {\n          \"Type\": Tag\n        }).then(response => {\n          console.log(response);\n        }).catch(response => {\n          console.log(response);\n        });\n      } //1.成功搜索到结果\n      //获取对象数组 显示相应结果\n\n      this.showSearch = true;\n      this.booklist.push({\n        bookid: 51,\n        bookname: 'Name1',\n        bookauthor: 'author' + 1,\n        bookpublisher: 'publisher' + 1,\n        booktag: Tag\n      }); //2.未找到\n      //this.showSearch=false;//不显示结果区域\n      //发送警告\n    },\n\n    searchUser(User) {\n      //方法：post  发送 userid 返回对象数组\n      //ajax request  search User\n      this.Userid = [];\n      this.booklist = []; // 清空当前显示的数据\n\n      {\n        //axios发送AJAX请求\n        this.axios.get('', {\n          \"Type\": User\n        }).then(response => {\n          console.log(response);\n        }).catch(response => {\n          console.log(response);\n        });\n      } //1.成功搜索到结果\n      //获取对象数组 显示相应结果\n\n      this.showSearch = true; //2.未找到\n      //this.showSearch=false;//不显示结果区域\n      //发送警告\n    }\n\n  },\n\n  setup() {\n    return {\n      options: [{\n        label: \"书名\",\n        key: \"书名\"\n      }, {\n        label: \"类型\",\n        key: \"类型\"\n      }, {\n        label: \"账号\",\n        key: \"账号\"\n      }]\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAsDA,SAAOA,eAAP,QAA6B,KAA7B;AACA,eAAeA,eAAe,CAAC;AAC/BC,MAAI,EAAC,aAD0B;AAE/BC,OAAK,EAAC,EAFyB;;AAG/BC,MAAI,GAAE;AAAC,WAAM;AACXC,gBAAU,EAAC,IADA;AAEXC,eAAS,EAAC,EAFC;AAGXC,cAAQ,EAAC,EAHE;AAIXC,cAAQ,EAAC,EAJE;AAKXC,gBAAU,EAAC,KALA;AAMXC,cAAQ,EAAC;AANE,KAAN;AAOL,GAV6B;;AAW/BC,WAAS,EAAC,EAXqB;AAY/BC,OAAK,EAAC,EAZyB;AAc/BC,SAAO,EAAC;AAGNC,gBAAY,CAACC,GAAD,EAAK;AACb,WAAKV,UAAL,GAAgBU,GAAhB;AACD,KALG;;AAQNC,eAAW,GAAE;AACZ,UAAG,KAAKX,UAAL,IAAiB,IAApB,EAAyB;AACvB,aAAKY,UAAL,CAAgB,KAAKX,SAArB;AACF,OAFA,MAGK,IAAG,KAAKD,UAAL,IAAiB,IAApB,EAAyB;AAC5B,aAAKa,SAAL,CAAe,KAAKZ,SAApB;AACF,OAFK,MAGA,IAAG,KAAKD,UAAL,IAAiB,IAApB,EAAyB;AAC1B,aAAKc,UAAL,CAAgB,KAAKb,SAArB;AACJ;AACA,KAlBK;;AAqBJ;;AACA;AACA;AACFW,cAAU,CAACG,IAAD,EAAM;AAAC;AACf;AAEA,WAAKC,MAAL,GAAY,EAAZ;AACA,WAAKd,QAAL,GAAc,EAAd,CAJc,CAIG;;AAEjB;AAAC;AACC,aAAKe,KAAL,CAAWC,GAAX,CAAe,EAAf,EACA;AAAC,kBAAOH;AAAR,SADA,EAEGI,IAFH,CAESC,Q