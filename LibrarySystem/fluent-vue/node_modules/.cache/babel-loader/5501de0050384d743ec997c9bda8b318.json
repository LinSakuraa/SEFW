{"ast":null,"code":"import { defineComponent } from \"vue\";\nexport default defineComponent({\n  name: 'SearchBar',\n  props: {},\n\n  data() {\n    return {\n      buttonText: '书名',\n      inputText: '',\n      booklist: [],\n      showSearch: false\n    };\n  },\n\n  component: {},\n  watch: {},\n  methods: {\n    handleSelect(key) {\n      this.buttonText = key;\n    },\n\n    submitInput() {\n      if (this.buttonText == '书名') {\n        this.searchName(this.inputText);\n      } else {\n        this.searchTag(this.inputText);\n      }\n    },\n\n    BorrowBook(bookid) {\n      if (this.$store.state.isLogin == false) {\n        alert('请先登录再借书！');\n      } else this.getBorrowBook(bookid);\n    },\n\n    //以下为涉及axios调用的方法\n\n    /****************************************************************/\n    //ajax请求 方法\n    searchName(Name) {\n      //方法：get  发送Name 返回对象数组\n      //ajax request: search bookName\n      this.booklist = []; //1.成功搜索到结果\n      //获取对象数组 显示相应结果\n\n      this.showSearch = true;\n      this.booklist.push({\n        bookid: 55,\n        bookname: Name,\n        bookauthor: 'author' + 1,\n        bookpublisher: 'publisher' + 1,\n        booktag: 'amusement'\n      }); //2.未找到\n      // this.showSearch=false;//不显示结果区域\n      //发送警告\n    },\n\n    searchTag(Tag) {\n      //方法：get  发送Name 返回对象数组\n      //ajax request  search bookTag\n      this.booklist = []; //1.成功搜索到结果\n      //获取对象数组 显示相应结果\n\n      this.showSearch = true;\n      this.booklist.push({\n        bookid: 51,\n        bookname: 'Name1',\n        bookauthor: 'author' + 1,\n        bookpublisher: 'publisher' + 1,\n        booktag: Tag\n      });\n      this.booklist.push({\n        bookid: 54,\n        bookname: 'Name2',\n        bookauthor: 'author' + 2,\n        bookpublisher: 'publisher' + 2,\n        booktag: Tag\n      }); //2.未找到\n      //this.showSearch=false;//不显示结果区域\n      //发送警告\n    },\n\n    getBorrowBook(bookid) {//方法：get  发送userid,bookid 返回借书情况:boolean\n    } //获取对象数组\n\n\n  },\n\n  setup() {\n    return {\n      options: [{\n        label: \"书名\",\n        key: \"书名\"\n      }, {\n        label: \"类型\",\n        key: \"类型\"\n      }]\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAoDA,SAAOA,eAAP,QAA6B,KAA7B;AACA,eAAeA,eAAe,CAAC;AAC/BC,MAAI,EAAC,WAD0B;AAE/BC,OAAK,EAAC,EAFyB;;AAG/BC,MAAI,GAAE;AAAC,WAAM;AACXC,gBAAU,EAAC,IADA;AAEXC,eAAS,EAAC,EAFC;AAGXC,cAAQ,EAAC,EAHE;AAIXC,gBAAU,EAAC;AAJA,KAAN;AAKL,GAR6B;;AAS/BC,WAAS,EAAC,EATqB;AAU/BC,OAAK,EAAC,EAVyB;AAY/BC,SAAO,EAAC;AAGNC,gBAAY,CAACC,GAAD,EAAK;AACb,WAAKR,UAAL,GAAgBQ,GAAhB;AACD,KALG;;AAQNC,eAAW,GAAE;AACZ,UAAG,KAAKT,UAAL,IAAiB,IAApB,EAAyB;AACvB,aAAKU,UAAL,CAAgB,KAAKT,SAArB;AACF,OAFA,MAGI;AACF,aAAKU,SAAL,CAAe,KAAKV,SAApB;AACF;AACA,KAfK;;AAgBNW,cAAU,CAACC,MAAD,EAAQ;AAChB,UAAG,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,OAAlB,IAA2B,KAA9B,EAAoC;AAClCC,aAAK,CAAC,UAAD,CAAL;AACD,OAFD,MAEM,KAAKC,aAAL,CAAmBL,MAAnB,CAAD;AACN,KApBK;;AAuBJ;;AACA;AACA;AACFH,cAAU,CAACS,IAAD,EAAM;AAAC;AACf;AAEA,WAAKjB,QAAL,GAAc,EAAd,CAHc,CAKd;AACA;;AACA,WAAKC,UAAL,GAAgB,IAAhB;AACA,WAAKD,QAAL,CAAckB,IAAd,CAAmB;AAACP,cAAM,EAAC,EAAR;AAAWQ,gBAAQ,EAACF,IAApB;AAAyBG,kBAAU,EAAC,WAAS,CAA7C;AAA+CC,qBAAa,EAAC,cAAY,CAAzE;AAA2EC,eAAO,EAAC;AAAnF,OAAnB,EARc,CAUd;AACA;AAGA;AACD,KAzCK;;AA2CNb,aAAS,CAACc,GAAD,EAAK;AAAC;AACb;AACA,WAAKvB,QAAL,GAAc,EAAd,CAFY,CAIf;AACG;;AACA,WAAKC,UAAL,GAAgB,IAAhB;AAEA,WAAKD,QAAL,CAAckB,IAAd,CAAmB;AAACP,cAAM,EAAC,EAAR;AAAWQ,gBAAQ,EAAC,OAApB;AAA4BC,kBAAU,EAAC,WAAS,CAAhD;AAAkDC,qBAAa,EAAC,cAAY,CAA5E;AAA8EC,eAAO,EAACC;AAAtF,OAAnB;AACA,WAAKvB,QAAL,CAAckB,IAAd,CAAmB;AAACP,cAAM,EAAC,EAAR;AAAWQ,gBAAQ,EAAC,OAApB;AAA4BC,kBAAU,EAAC,WAAS,CAAhD;AAAkDC,qBAAa,EAAC,cAAY,CAA5E;AAA8EC,eAAO,EAACC;AAAtF,OAAnB,EATY,CAWZ;AACE;AAGF;AACD,KA3DK;;AA4DNP,iBAAa,CAACL,MAAD,EAAQ,CAAC;AAEtB,KA9DM,CAgEJ;;;AAhEI,GAZuB;;AA8E/Ba,OAAK,GAAE;AACL,WAAM;AACLC,aAAO,EAAC,CAAC;AAACC,aAAK,EAAC,IAAP;AAAYpB,WAAG,EAAC;AAAhB,OAAD,EACT;AAACoB,aAAK,EAAC,IAAP;AAAYpB,WAAG,EAAC;AAAhB,OADS;AADH,KAAN;AAKF;;AApF+B,CAAD,CAA9B","names":["defineComponent","name","props","data","buttonText","inputText","booklist","showSearch","component","watch","methods","handleSelect","key","submitInput","searchName","searchTag","BorrowBook","bookid","$store","state","isLogin","alert","getBorrowBook","Name","push","bookname","bookauthor","bookpublisher","booktag","Tag","setup","options","label"],"sourceRoot":"","sources":["G:\\VUE_FDS\\fluent-vue\\src\\components\\SearchBar.vue"],"sourcesContent":["<template>\r\n  <div class=\"div-SearchBar\">\r\n  <div class=\"Search-handle\">\r\n    <div class=\"choose-div\">\r\n      <n-dropdown trigger=\"hover\" :options=\"options\" @select=\"handleSelect\">\r\n    <n-button class=\"choose-button\" color=\"#6ac5ec\">{{buttonText}}</n-button>\r\n  </n-dropdown>\r\n    </div>\r\n    <div class=\"input-div\">\r\n      <input v-model=\"inputText\" class=\"input-bar\"/>\r\n    </div>\r\n    <div class=\"button-div\">\r\n        <n-button class=\"submit-button\" @click=\"submitInput\" color=\"#6ac5ec\">搜索</n-button>\r\n      </div>\r\n  </div>\r\n  </div>\r\n\r\n\r\n  <div class=\"search-div-book\" v-show=\"showSearch\">\r\n          <div class=\"search-book-list\">\r\n          <div class=\"search-book-container\"  v-for=\"book in booklist\" :key=\"book.bookid\">\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">书籍名称</div>\r\n                <div class=\"search-book-showright\">{{book.bookname}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">书籍编号</div>\r\n                <div class=\"search-book-showright\">{{book.bookid}}</div>\r\n             </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">书籍作者</div>\r\n                <div class=\"search-book-showright\">{{book.bookauthor}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">出版社</div>\r\n                <div class=\"search-book-showright\">{{book.bookpublisher}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">类型</div>\r\n            <div class=\"search-book-showright\">{{book.booktag}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n              <div style=\"display:flex;justify-content:center;align-items:center; width:100%; height:100%;\">\r\n              <button style=\"height:30px; width:60px\" @click=\"BorrowBook(book.bookid)\">借书</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          </div>\r\n        </div>\r\n</template>\r\n\r\n<script>\r\nimport{defineComponent} from \"vue\";\r\nexport default defineComponent({\r\nname:'SearchBar',\r\nprops:{},\r\ndata(){return{\r\n  buttonText:'书名',\r\n  inputText:'',\r\n  booklist:[],\r\n  showSearch:false,\r\n}},\r\ncomponent:{},\r\nwatch:{\r\n},\r\nmethods:{\r\n\r\n\r\n  handleSelect(key){\r\n      this.buttonText=key;\r\n    },\r\n\r\n\r\n  submitInput(){\r\n   if(this.buttonText=='书名'){\r\n     this.searchName(this.inputText)\r\n   }\r\n   else{\r\n     this.searchTag(this.inputText)\r\n   }\r\n  },\r\n  BorrowBook(bookid){\r\n    if(this.$store.state.isLogin==false){\r\n      alert('请先登录再借书！');\r\n    }else(this.getBorrowBook(bookid))\r\n  },\r\n\r\n\r\n    //以下为涉及axios调用的方法\r\n    /****************************************************************/\r\n    //ajax请求 方法\r\n  searchName(Name){//方法：get  发送Name 返回对象数组\r\n    //ajax request: search bookName\r\n\r\n    this.booklist=[];\r\n\r\n    //1.成功搜索到结果\r\n    //获取对象数组 显示相应结果\r\n    this.showSearch=true\r\n    this.booklist.push({bookid:55,bookname:Name,bookauthor:'author'+1,bookpublisher:'publisher'+1,booktag:'amusement'})\r\n\r\n    //2.未找到\r\n    // this.showSearch=false;//不显示结果区域\r\n\r\n\r\n    //发送警告\r\n  },\r\n\r\n  searchTag(Tag){//方法：get  发送Name 返回对象数组\r\n    //ajax request  search bookTag\r\n    this.booklist=[];\r\n\r\n //1.成功搜索到结果\r\n    //获取对象数组 显示相应结果\r\n    this.showSearch=true\r\n    \r\n    this.booklist.push({bookid:51,bookname:'Name1',bookauthor:'author'+1,bookpublisher:'publisher'+1,booktag:Tag})\r\n    this.booklist.push({bookid:54,bookname:'Name2',bookauthor:'author'+2,bookpublisher:'publisher'+2,booktag:Tag})\r\n\r\n    //2.未找到\r\n      //this.showSearch=false;//不显示结果区域\r\n\r\n\r\n    //发送警告\r\n  },\r\n  getBorrowBook(bookid){//方法：get  发送userid,bookid 返回借书情况:boolean\r\n    \r\n  }\r\n\r\n    //获取对象数组\r\n  },\r\nsetup(){\r\n  return{\r\n   options:[{label:\"书名\",key:\"书名\"},\r\n  {label:\"类型\",key:\"类型\"}\r\n    ],\r\n  }\r\n}\r\n}\r\n)\r\n</script>\r\n\r\n<style>\r\n.div-SearchBar{\r\n  display: flex;\r\n  height:auto;\r\n  width:100%;\r\n  justify-content: center;\r\n}\r\n.Search-handle{\r\n  margin-top:10px;\r\n  display:flex;\r\n  height:40px;\r\n  width:480px;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.choose-div{\r\nheight:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.choose-button{\r\n  height:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.input-div{\r\nheight:40px;\r\nwidth:300px;\r\npadding-left:20px;\r\n}\r\n.input-bar{\r\n  height:38px;\r\nwidth:300px;\r\npadding:0;\r\npadding-left:10px;\r\nborder:1px solid #b0b3b8;\r\n\r\n}\r\n\r\n.button-div{\r\nmargin-left:10px;\r\nheight:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.submit-button{\r\nheight:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.search-div-book{\r\nheight:auto;\r\ndisplay:flex;\r\njustify-content:center;\r\nalign-items:center;\r\nwidth:100%\r\n}\r\n.search-book-list{\r\nwidth:800px;\r\ndisplay:flex;\r\n  flex-wrap:wrap ;\r\n}\r\n.search-book-container{\r\n  display:flex;\r\n  width:240px;\r\n  height:240px;\r\n  flex-direction:column;\r\n  border-radius: 10px;\r\n  background-color:#fafafa;\r\n  margin:10px;\r\n\r\n}\r\n.search-book-show{\r\n  width:100%;\r\n  height:40px;\r\n   display:flex;\r\n}\r\n.search-book-showleft{\r\nwidth:70px;\r\nheight:100%;\r\npadding-left:10px;\r\ndisplay:flex;\r\njustify-content:center;\r\nalign-items:center;\r\n}\r\n.search-book-showright{\r\nwidth:140px;\r\nheight:100%;\r\npadding-left:10px;\r\ndisplay:flex;\r\njustify-content:center;\r\nalign-items:center;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}