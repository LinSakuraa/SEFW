context, definition)}\n        </div>\n        <div\n            class=\"listbox\"\n            id=\"${x => x.listboxId}\"\n            part=\"listbox\"\n            role=\"listbox\"\n            ?disabled=\"${x => x.disabled}\"\n            ?hidden=\"${x => !x.open}\"\n            ${ref(\"listbox\")}\n        >\n            <slot\n                ${slotted({\n    filter: Listbox.slottedOptionFilter,\n    flatten: true,\n    property: \"slottedOptions\",\n})}\n            ></slot>\n        </div>\n    </template>\n`;\n"]},"metadata":{},"sourceType":"module"}