onse)=>{\r\n        console.log( response)\r\n      })\r\n    }\r\n\r\n //1.成功搜索到结果\r\n    //获取对象数组 显示相应结果\r\n    this.showSearch=true\r\n    \r\n    this.booklist.push({tagi:0,bookid:51,bookname:'Name1',bookauthor:'author'+1,bookpublisher:'publisher'+1,booktag:Tag,inputshow:false})\r\n\r\n    //2.未找到\r\n      //this.showSearch=false;//不显示结果区域\r\n\r\n\r\n    //发送警告\r\n  },\r\n\r\n  searchUser(User){//方法：post  发送 userid 返回对象数组\r\n    //ajax request  search User\r\n\r\n    this.userlist=[];\r\n    this.booklist=[];// 清空当前显示的数据\r\n\r\n    {//axios发送AJAX请求\r\n      this.axios.get('',\r\n      {\"Type\":User\r\n      }).then((response)=>{\r\n        console.log( response)\r\n\r\n      }).catch((response)=>{\r\n        console.log( response)\r\n      })\r\n    }\r\n\r\n    this.showSearch=true;\r\n\r\n     this.userlist.push({tagi:0,userid:User,username:'自豪简一郎',password:'114514'})\r\n\r\n //1.成功搜索到结果\r\n    //获取对象数组 显示相应结果\r\n    this.showSearch=true\r\n\r\n    //2.未找到\r\n    //this.showSearch=false;//不显示结果区域\r\n\r\n\r\n    //发送警告\r\n  },\r\n\r\n\r\n\r\n  //change function\r\n  //管理员修改相关方法\r\n  ChangeBook(tagi){\r\n    this.thisbook=this.booklist[tagi];\r\n    this.showModalBook=!this.showModalBook;\r\n   console.log(this.booklist[tagi]);\r\n    console.log(this.showModalBook);\r\n  },\r\n  DeleteBook(bookid){\r\n\r\n  },\r\n  ChangeUser(tagi){\r\n\r\n  },\r\n  DeleteUser(userid){\r\n\r\n  }\r\n  },\r\nsetup(){\r\n  return{\r\n   options:[{label:\"书名\",key:\"书名\"},\r\n  {label:\"类型\",key:\"类型\"},\r\n  {label:\"账号\",key:\"账号\"}\r\n    ],\r\n  }\r\n}\r\n}\r\n)\r\n</script>\r\n\r\n<style>\r\n.div-SearchBar{\r\n  display: flex;\r\n  height:auto;\r\n  width:100%;\r\n  justify-content: center;\r\n}\r\n.Search-handle{\r\n  margin-top:10px;\r\n  display:flex;\r\n  height:40px;\r\n  width:480px;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.choose-div{\r\nheight:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\