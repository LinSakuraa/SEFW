{"ast":null,"code":"import { defineComponent } from \"vue\";\nexport default defineComponent({\n  name: 'SearchBar',\n  props: {},\n\n  data() {\n    return {\n      buttonText: '书名',\n      inputText: '',\n      booklist: []\n    };\n  },\n\n  component: {},\n  watch: {},\n  methods: {\n    handleSelect(key) {\n      this.buttonText = key;\n    },\n\n    submitInput() {\n      if (this.buttonText == '书名') {\n        this.searchName(this.inputText);\n      } else {\n        this.searchTag(this.inputText);\n      }\n    },\n\n    searchName(Name) {\n      //ajax request: search bookName\n      alert(\"bookName:\" + Name);\n    },\n\n    searchTag(Tag) {\n      //ajax request  search bookTag\n      alert(\"bookTag:\" + Tag);\n    }\n\n  },\n\n  setup() {\n    return {\n      options: [{\n        label: \"书名\",\n        key: \"书名\"\n      }, {\n        label: \"类型\",\n        key: \"类型\"\n      }]\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAoDA,SAAOA,eAAP,QAA6B,KAA7B;AACA,eAAeA,eAAe,CAAC;AAC/BC,MAAI,EAAC,WAD0B;AAE/BC,OAAK,EAAC,EAFyB;;AAG/BC,MAAI,GAAE;AAAC,WAAM;AACXC,gBAAU,EAAC,IADA;AAEXC,eAAS,EAAC,EAFC;AAGXC,cAAQ,EAAC;AAHE,KAAN;AAIL,GAP6B;;AAQ/BC,WAAS,EAAC,EARqB;AAS/BC,OAAK,EAAC,EATyB;AAW/BC,SAAO,EAAC;AAGNC,gBAAY,CAACC,GAAD,EAAK;AACb,WAAKP,UAAL,GAAgBO,GAAhB;AACD,KALG;;AAQNC,eAAW,GAAE;AACZ,UAAG,KAAKR,UAAL,IAAiB,IAApB,EAAyB;AACvB,aAAKS,UAAL,CAAgB,KAAKR,SAArB;AACF,OAFA,MAGI;AACF,aAAKS,SAAL,CAAe,KAAKT,SAApB;AACF;AACA,KAfK;;AAkBNQ,cAAU,CAACE,IAAD,EAAM;AACd;AACAC,WAAK,CAAC,cAAYD,IAAb,CAAL;AACD,KArBK;;AAuBND,aAAS,CAACG,GAAD,EAAK;AACZ;AACAD,WAAK,CAAC,aAAWC,GAAZ,CAAL;AACD;;AA1BK,GAXuB;;AA0C/BC,OAAK,GAAE;AACL,WAAM;AACLC,aAAO,EAAC,CAAC;AAACC,aAAK,EAAC,IAAP;AAAYT,WAAG,EAAC;AAAhB,OAAD,EACT;AAACS,aAAK,EAAC,IAAP;AAAYT,WAAG,EAAC;AAAhB,OADS;AADH,KAAN;AAKF;;AAhD+B,CAAD,CAA9B","names":["defineComponent","name","props","data","buttonText","inputText","booklist","component","watch","methods","handleSelect","key","submitInput","searchName","searchTag","Name","alert","Tag","setup","options","label"],"sourceRoot":"","sources":["G:\\VUE_FDS\\fluent-vue\\src\\components\\SearchBar.vue"],"sourcesContent":["<template>\r\n  <div class=\"div-SearchBar\">\r\n  <div class=\"Search-handle\">\r\n    <div class=\"choose-div\">\r\n      <n-dropdown trigger=\"hover\" :options=\"options\" @select=\"handleSelect\">\r\n    <n-button class=\"choose-button\" color=\"#6ac5ec\">{{buttonText}}</n-button>\r\n  </n-dropdown>\r\n    </div>\r\n    <div class=\"input-div\">\r\n      <input v-model=\"inputText\" class=\"input-bar\"/>\r\n    </div>\r\n    <div class=\"button-div\">\r\n        <n-button class=\"submit-button\" @click=\"submitInput\" color=\"#6ac5ec\">搜索</n-button>\r\n      </div>\r\n  </div>\r\n  </div>\r\n\r\n\r\n  <div class=\"search-div-book\" v-if=\"showChoose==true\">\r\n          <div class=\"search-book-list\">\r\n          <div class=\"search-book-container\"  v-for=\"book in booklist\" :key=\"book.bookid\">\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">书籍名称</div>\r\n                <div class=\"search-book-showright\">{{book.bookname}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">书籍编号</div>\r\n                <div class=\"search-book-showright\">{{book.bookid}}</div>\r\n             </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">书籍作者</div>\r\n                <div class=\"search-book-showright\">{{book.bookauthor}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">出版社</div>\r\n                <div class=\"search-book-showright\">{{book.bookpublisher}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n                <div class=\"search-book-showleft\">类型</div>\r\n            <div class=\"search-book-showright\">{{book.booktag}}</div>\r\n            </div>\r\n            <div class=\"search-book-show\">\r\n              <div style=\"display:flex;justify-content:center;align-items:center; width:100%; height:100%;\">\r\n              <button style=\"height:30px; width:60px\" @click=\"returnBook(book.bookid)\">还书</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          </div>\r\n        </div>\r\n</template>\r\n\r\n<script>\r\nimport{defineComponent} from \"vue\";\r\nexport default defineComponent({\r\nname:'SearchBar',\r\nprops:{},\r\ndata(){return{\r\n  buttonText:'书名',\r\n  inputText:'',\r\n  booklist:[]\r\n}},\r\ncomponent:{},\r\nwatch:{\r\n},\r\nmethods:{\r\n\r\n\r\n  handleSelect(key){\r\n      this.buttonText=key;\r\n    },\r\n\r\n\r\n  submitInput(){\r\n   if(this.buttonText=='书名'){\r\n     this.searchName(this.inputText)\r\n   }\r\n   else{\r\n     this.searchTag(this.inputText)\r\n   }\r\n  },\r\n\r\n\r\n  searchName(Name){\r\n    //ajax request: search bookName\r\n    alert(\"bookName:\"+Name)\r\n  },\r\n\r\n  searchTag(Tag){\r\n    //ajax request  search bookTag\r\n    alert(\"bookTag:\"+Tag)\r\n  },\r\n\r\n\r\n\r\n  },\r\nsetup(){\r\n  return{\r\n   options:[{label:\"书名\",key:\"书名\"},\r\n  {label:\"类型\",key:\"类型\"}\r\n    ],\r\n  }\r\n}\r\n}\r\n)\r\n</script>\r\n\r\n<style>\r\n.div-SearchBar{\r\n  display: flex;\r\n  height:auto;\r\n  width:100%;\r\n  justify-content: center;\r\n}\r\n.Search-handle{\r\n  margin-top:10px;\r\n  display:flex;\r\n  height:40px;\r\n  width:460px;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.choose-div{\r\nheight:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.choose-button{\r\n  height:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.input-div{\r\nheight:40px;\r\nwidth:300px;\r\npadding-left:20px;\r\n}\r\n.input-bar{\r\n  height:38px;\r\nwidth:300px;\r\npadding:0;\r\nborder:1px solid #b0b3b8;\r\n\r\n}\r\n\r\n.button-div{\r\nheight:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.submit-button{\r\nheight:40px;\r\nwidth:60px;\r\ndisplay:flex;\r\njustify-content: center;\r\nalign-items: center;\r\n}\r\n\r\n.search-div-book{\r\nheight:auto;\r\ndisplay:flex;\r\njustify-content:center;\r\nalign-items:center;\r\nwidth:100%\r\n}\r\n.search-book-list{\r\nwidth:800px;\r\ndisplay:flex;\r\n  flex-wrap:wrap ;\r\n}\r\n.search-book-container{\r\n  display:flex;\r\n  width:240px;\r\n  height:240px;\r\n  flex-direction:column;\r\n  border-radius: 10px;\r\n  background-color:#fafafa;\r\n  margin:10px;\r\n\r\n}\r\n.search-book-show{\r\n  width:100%;\r\n  height:40px;\r\n   display:flex;\r\n}\r\n.search-book-showleft{\r\nwidth:70px;\r\nheight:100%;\r\npadding-left:10px;\r\ndisplay:flex;\r\njustify-content:center;\r\nalign-items:center;\r\n}\r\n.search-book-showright{\r\nwidth:140px;\r\nheight:100%;\r\npadding-left:10px;\r\ndisplay:flex;\r\njustify-content:center;\r\nalign-items:center;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}