   this.axios.get('', {\n          \"Type\": Tag\n        }).then(response => {\n          console.log(response);\n        }).catch(response => {\n          console.log(response);\n        });\n      } //1.成功搜索到结果\n      //获取对象数组 显示相应结果\n\n      this.showSearch = true;\n      this.booklist.push({\n        tagi: 0,\n        bookid: 51,\n        bookname: 'Name1',\n        bookauthor: 'author' + 1,\n        bookpublisher: 'publisher' + 1,\n        booktag: Tag,\n        inputshow: false\n      }); //2.未找到\n      //this.showSearch=false;//不显示结果区域\n      //发送警告\n    },\n\n    searchUser(User) {\n      //方法：post  发送 userid 返回对象数组\n      //ajax request  search User\n      this.userlist = [];\n      this.booklist = []; // 清空当前显示的数据\n\n      {\n        //axios发送AJAX请求\n        this.axios.get('', {\n          \"Type\": User\n        }).then(response => {\n          console.log(response);\n        }).catch(response => {\n          console.log(response);\n        });\n      }\n      this.showSearch = true;\n      this.userlist.push({\n        tagi: 0,\n        userid: User,\n        username: '自豪简一郎',\n        password: '114514'\n      }); //1.成功搜索到结果\n      //获取对象数组 显示相应结果\n\n      this.showSearch = true; //2.未找到\n      //this.showSearch=false;//不显示结果区域\n      //发送警告\n    },\n\n    //change function\n    //管理员修改相关方法\n    ChangeBook(tagi) {\n      this.thisbook = this.booklist[tagi];\n      this.showModalBook = !this.showModalBook;\n      console.log(this.booklist[tagi]);\n      console.log(this.showModalBook);\n    },\n\n    DeleteBook(bookid) {},\n\n    ChangeUser(tagi) {},\n\n    DeleteUser(userid) {}\n\n  },\n\n  setup() {\n    return {\n      options: [{\n        label: \"书名\",\n        key: \"书名\"\n      }, {\n        label: \"类型\",\n        key: \"类型\"\n      }, {\n        label: \"账号\",\n        key: \"账号\"\n      }]\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAyFA,SAAOA,eAAP,QAA6B,KAA7B;AACA,OAAOC,WAAP,MAAwB,mCAAxB;AACA,eAAeD,eAAe,CAAC;AAC/BE,MAAI,EAAC,aAD0B;